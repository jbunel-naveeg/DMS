{
  "version": 2,
  "builds": [
    {
      "src": "apps/marketing/package.json",
      "use": "@vercel/next",
      "config": {
        "distDir": ".next"
      }
    },
    {
      "src": "apps/dashboard/package.json",
      "use": "@vercel/next",
      "config": {
        "distDir": ".next"
      }
    }
  ],
  "routes": [
    {
      "src": "/marketing/(.*)",
      "dest": "/apps/marketing/$1"
    },
    {
      "src": "/dashboard/(.*)",
      "dest": "/apps/dashboard/$1"
    }
  ]
}
