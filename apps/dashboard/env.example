# Dashboard App Environment Configuration
# Copy this file to .env.local and fill in your values

# =============================================================================
# CORE CONFIGURATION
# =============================================================================

# Application URLs
NEXT_PUBLIC_APP_URL=https://app.naveeg.com
NEXT_PUBLIC_MARKETING_URL=https://naveeg.com
NEXT_PUBLIC_TRIAL_DOMAIN=naveeg.online
NEXT_PUBLIC_APP_DOMAIN=app.naveeg.com

# Environment
NODE_ENV=development
NEXT_PUBLIC_VERCEL_ENV=development

# =============================================================================
# SUPABASE CONFIGURATION
# =============================================================================

# Supabase Project Configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Supabase Database Configuration
SUPABASE_DB_URL=postgresql://postgres:password@db.your-project.supabase.co:5432/postgres
SUPABASE_DB_PASSWORD=your_database_password

# Supabase Edge Functions
SUPABASE_FUNCTIONS_URL=https://your-project.supabase.co/functions/v1

# =============================================================================
# STRIPE CONFIGURATION
# =============================================================================

# Stripe API Keys (Test Mode)
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key
STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret

# Stripe Configuration
STRIPE_WEBHOOK_TOLERANCE=300
STRIPE_API_VERSION=2023-10-16

# Stripe Products (Test Mode)
STRIPE_STARTER_PRICE_ID=price_test_starter
STRIPE_PRO_PRICE_ID=price_test_pro

# =============================================================================
# 10WEB API CONFIGURATION
# =============================================================================

# 10Web API Configuration
TENWEB_API_KEY=your_10web_api_key
TENWEB_API_URL=https://api.10web.io
TENWEB_WEBHOOK_SECRET=your_10web_webhook_secret

# 10Web Default Settings
TENWEB_DEFAULT_TEMPLATE=business
TENWEB_DEFAULT_PLAN=basic

# =============================================================================
# OPENAI CONFIGURATION
# =============================================================================

# OpenAI API Configuration
OPENAI_API_KEY=sk-your_openai_api_key
OPENAI_ORGANIZATION=org-your_organization_id
OPENAI_MODEL=gpt-4
OPENAI_MAX_TOKENS=2000
OPENAI_TEMPERATURE=0.7

# =============================================================================
# GOOGLE OAUTH CONFIGURATION
# =============================================================================

# Google OAuth Configuration
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
GOOGLE_REDIRECT_URI=https://app.naveeg.com/api/google/oauth/callback

# Google Analytics Configuration
GOOGLE_ANALYTICS_PROPERTY_ID=your_ga_property_id
GOOGLE_ANALYTICS_MEASUREMENT_ID=G-XXXXXXXXXX

# Google Search Console Configuration
GOOGLE_SEARCH_CONSOLE_SITE_URL=https://your-domain.com

# Google Business Profile Configuration
GOOGLE_BUSINESS_PROFILE_ACCOUNT_ID=your_business_profile_account_id

# =============================================================================
# N8N AUTOMATION CONFIGURATION
# =============================================================================

# N8N Configuration
N8N_WEBHOOK_URL=https://your_n8n_instance.com
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=your_n8n_basic_auth_password
N8N_ENCRYPTION_KEY=your_32_character_encryption_key

# N8N Workflow URLs
N8N_LEAD_MANAGEMENT_WEBHOOK=https://your_n8n_instance.com/webhook/lead-management
N8N_SITE_PROVISIONING_WEBHOOK=https://your_n8n_instance.com/webhook/site-provisioning
N8N_SUBSCRIPTION_WEBHOOK=https://your_n8n_instance.com/webhook/subscription-management
N8N_DOMAIN_WEBHOOK=https://your_n8n_instance.com/webhook/domain-management

# =============================================================================
# NOTIFICATION CONFIGURATION
# =============================================================================

# Slack Configuration
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/SLACK/WEBHOOK
SLACK_CHANNEL=#naveeg-notifications
SLACK_BOT_TOKEN=xoxb-your_slack_bot_token

# Email Configuration (Optional - using Supabase Auth)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your_email@gmail.com
SMTP_PASSWORD=your_app_password
SMTP_FROM=noreply@naveeg.com

# =============================================================================
# SECURITY CONFIGURATION
# =============================================================================

# JWT Configuration
JWT_SECRET=your_jwt_secret_key
JWT_EXPIRES_IN=7d

# Encryption Configuration
ENCRYPTION_KEY=your_32_character_encryption_key
ENCRYPTION_ALGORITHM=aes-256-gcm

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# =============================================================================
# MONITORING & ANALYTICS
# =============================================================================

# Sentry Configuration (Optional)
SENTRY_DSN=https://your_sentry_dsn@sentry.io/project_id
SENTRY_ORG=your_sentry_org
SENTRY_PROJECT=your_sentry_project

# Vercel Analytics
VERCEL_ANALYTICS_ID=your_vercel_analytics_id

# =============================================================================
# FEATURE FLAGS
# =============================================================================

# Feature Toggles
ENABLE_AI_CHATBOT=true
ENABLE_GOOGLE_INTEGRATIONS=true
ENABLE_TEAM_COLLABORATION=true
ENABLE_CUSTOM_DOMAINS=true
ENABLE_ANALYTICS=true
ENABLE_N8N_WORKFLOWS=true

# =============================================================================
# DEVELOPMENT CONFIGURATION
# =============================================================================

# Debug Configuration
DEBUG=naveeg:*
LOG_LEVEL=info
ENABLE_DEBUG_ROUTES=false

# Database Configuration
DB_POOL_SIZE=10
DB_CONNECTION_TIMEOUT=30000
DB_QUERY_TIMEOUT=30000

# Cache Configuration
REDIS_URL=redis://localhost:6379
CACHE_TTL=3600
